        "html2canvas": "^1.0.0-rc.3",
        "moment": "^2.24.0",
        "mui-downshift": "^1.4.1",
        "prop-types": "^15.7.2",
        "react": "^16.8.6",
        "react-big-calendar": "^0.19.2",
        "react-color": "^2.17.3",
        "react-dom": "^16.8.6",
        "react-ga": "^2.5.7",
        "react-scripts": "^2.1.8",
        "react-input-mask": "^2.0.4",
        "recharts": "^1.6.2",
        "react-device-detect": "^1.11.14",
        "@john-osullivan/react-window-dynamic-fork": "^1.9.0-alpha.1",
        "react-virtualized-auto-sizer": "^1.0.2",
        "material-ui-popup-state": "^1.4.0",
        "flux": "^3.1.3"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "postbuild": "react-snap",
        "nodemon": "^1.14.6",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject"
    },
    "devDependencies": {
        "husky": "^2.4.1",
        "lint-staged": "^8.2.1",
        "prettier": "^1.18.2",
        "react-snap": "^1.23.0"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "reactSnap": {
        "inlineCss": true
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,json,css,html}": [
            "prettier --write",
            "git add"
        ]
    },
    "prettier": {
        "printWidth": 80,
        "tabWidth": 4,
        "useTabs": false,
        "semi": true,
        "singleQuote": true,
        "quoteProps": "as-needed",
        "jsxSingleQuote": false,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "arrowParens": "always",
        "endOfLine": "lf"
    },
    "proxy": "http://localhost:8080"
}
